configs {
  key: "medical-ai.build"
  value {
    requirement {
      cpu: 4
      memory: 8
      image: "docker"
    }
    time_limit {
      seconds: 1800
    }
    command:
      "docker run -v $PWD/build.sh:/build.sh -v $PWD/deploy.sh:/deploy.sh -e GH_TOKEN=$GH_TOKEN --rm -t mitmul/medical-ai-course-materials:build bash -c 'bash build.sh && bash deploy.sh'\n"
  }
}
